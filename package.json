{
  "name": "kickstart-react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run typecheck && vite build",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint ./src",
    "fmt": "dprint fmt",
    "fmt:check": "dprint check",
    "model:generate": "npx tsx ./scripts/generateModel.ts",
    "model:import": "npx tsx ./scripts/importProject.ts",
    "preview": "vite preview"
  },
  "dependencies": {
    "@kontent-ai/delivery-sdk": "^16.3.0",
    "@kontent-ai/rich-text-resolver": "^2.0.4",
    "@portabletext/vue": "^1.0.14",
    "@tailwindcss/vite": "^4.1.13",
    "@tanstack/vue-query": "^5.90.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "dotenv": "^17.2.2",
    "pinia": "^3.0.3",
    "tailwindcss": "^4.1.11",
    "vue": "^3.5.20",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@kontent-ai/data-ops": "^2.3.13",
    "@kontent-ai/model-generator": "^9.1.0",
    "@types/node": "^24.5.2",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "dprint": "^0.49.1",
    "eslint": "^9.25.1",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.0.0",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.31.0",
    "vite": "^7.1.11",
    "vue-tsc": "^3.0.6"
  }
}
